<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="258.0" prefWidth="498.0" spacing="10.0" style="-fx-background-color: #836FFF;" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.edu.ifsc.canoinhas.poo2.NN.controllers.NoticiasController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <Label fx:id="lblUser" text="Label" />
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <TableView fx:id="listNew" prefHeight="247.0" prefWidth="461.0" HBox.hgrow="ALWAYS">
              <columns>
                <TableColumn fx:id="ID" prefWidth="75.0" text="ID" />
                <TableColumn fx:id="Titulo" prefWidth="380.0" text="Noticias" />
              </columns>
            </TableView>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="btNewGenero" mnemonicParsing="false" onAction="#ANovoGenero" text="ADD Genero" />
            <Button fx:id="btnPerfil" mnemonicParsing="false" onAction="#APerfil" text="Perfil" />
            <Button fx:id="btnAN" mnemonicParsing="false" onAction="#AAlterar" text="Alterar Noticia" />
            <Button fx:id="btnRN" mnemonicParsing="false" onAction="#ARemover" text="Remover Noticia" />
            <Button fx:id="btnAddN" mnemonicParsing="false" onAction="#AAdicionar" text="Adicionar Noticia" />
            <Button fx:id="btnSair" mnemonicParsing="false" onAction="#ASair" text="Sair" />
         </children>
      </HBox>
   </children>
</VBox>
